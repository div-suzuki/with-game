<div class="contents row">
  <div class="container">
    <h3>新規募集ルーム作成</h3>
  <%= form_with(model: @room, local: true) do |form| %>      
    <%= render 'layouts/error_messages', model: form.object %>  
    <%= form.text_field :tag_id, placeholder: "タグ付け" %>
    <%= form.text_field :member, placeholder: "募集人数" %>
    <%= form.text_area :text, placeholder: "備考" %>
    <%= form.submit "募集" %>
  <% end %>
  </div>
</div>